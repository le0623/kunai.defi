{
  "name": "kunai-backend",
  "version": "1.0.0",
  "description": "Kunai Trading Platform Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "bot": "tsx src/bot.ts",
    "bot:dev": "tsx watch src/bot.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "trading",
    "ethereum",
    "defi",
    "api"
  ],
  "author": "Kunai Team",
  "license": "MIT",
  "dependencies": {
    "@kunai/shared": "workspace:*",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@telegram-apps/init-data-node": "^2.0.10",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.4",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moralis": "^2.27.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.5",
    "prisma": "^6.10.1",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "telegraf": "^4.16.3",
    "viem": "^2.31.3",
    "web3": "^4.16.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "jest": "^30.0.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}